{
  "name": "raphael",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "jill run 'prisma:generate && prisma:deploy && build:next'",
    "build:next": "next build",
    "codegen": "dotenvx run --ignore=MISSING_ENV_FILE -- graphql-codegen -c graphql.config.yml",
    "codegen:check": "yarn codegen --check",
    "codegen:watch": "yarn codegen --watch",
    "dev": "next dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "dotenvx run --ignore=MISSING_ENV_FILE -- eslint",
    "prisma": "dotenvx run --ignore=MISSING_ENV_FILE -- prisma",
    "prisma:deploy": "yarn prisma migrate deploy",
    "prisma:generate": "yarn prisma generate",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "4.0.11",
    "@apollo/client-integration-nextjs": "0.14.4",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/material": "7.3.7",
    "@mui/material-nextjs": "7.3.7",
    "@octokit/auth-app": "8.1.2",
    "@octokit/webhooks": "9.26.3",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sentry/nextjs": "10.32.1",
    "@vercel/analytics": "1.6.1",
    "graphql": "16.12.0",
    "kyrielle": "2.8.3",
    "next": "16.1.1",
    "pg": "8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.51.4",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-operations": "5.0.7",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@jujulego/jill": "3.0.7",
    "@microsoft/eslint-formatter-sarif": "3.1.0",
    "@parcel/watcher": "2.5.1",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "24.10.4",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "10.1.8",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "prisma": "7.2.0",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.12.0"
